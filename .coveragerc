[run]
branch = True
include =
        ./*
omit =
        *tests*
        *__init__*
        *migrations*
        *asgi*
        *settings*
        *wsgi*
        *manage*
        *apps*
	*serializer*
[report]
exclude_lines = 
        pragma: no cover
        if __name__ == .__main__.:
[html]
directory = htmlcov
